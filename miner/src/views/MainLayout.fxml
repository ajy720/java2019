<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="views.MainController">
   <children>
      <HBox layoutX="14.0" layoutY="14.0" prefHeight="100.0" prefWidth="200.0" spacing="10.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
         <children>
            <Canvas fx:id="canvas" height="380.0" onMouseClicked="#clickHandle" width="450.0" />
            <VBox prefHeight="380.0" prefWidth="105.0" spacing="10.0" HBox.hgrow="ALWAYS">
               <children>
                  <Button fx:id="btnStart" mnemonicParsing="false" onAction="#restart" prefHeight="47.0" prefWidth="120.0" text="게임시작" />
                  <Label fx:id="lblFlag" alignment="CENTER" prefHeight="62.0" prefWidth="123.0" text="남은 깃발 : 10">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
